SELECT 
    e.left_operand,
    e.operator,
    e.right_operand,
    IF(CASE WHEN TRIM(e.operator) ='>' THEN v.value>v2.value
    WHEN TRIM(e.operator)='<' THEN v.value<v2.value
    WHEN TRIM(e.operator)='=' THEN v.value=v2.value END,'true','false') as value
FROM Variables v
INNER JOIN Expressions e
ON e.left_operand=v.name 
INNER JOIN Variables v2
ON e.right_operand=v2.name;
